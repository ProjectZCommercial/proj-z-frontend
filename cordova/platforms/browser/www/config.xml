<?xml version='1.0' encoding='utf-8'?>
<widget id="com.commercial.projz" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ProjZ</name>
    <description>
        Proj Z commercial
    </description>
    <author email="project.z.commercial@gmail.com" href="https://proj.z">
        ProjZ
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <hook src="../scripts/prepareAngular5App.js" type="before_prepare" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
</widget>
